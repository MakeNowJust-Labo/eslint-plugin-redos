{
  "name": "eslint-plugin-redos",
  "description": "ESLint plugin for catching ReDoS vulnerability.",
  "license": "MIT",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "version": "0.2.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "prettier --list-different *.{json,md} src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@makenowjust-labo/redos": "^0.2.0",
    "inflected": "^2.1.0"
  },
  "peerDependencies": {
    "eslint": ">= 7"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.4",
    "@types/inflected": "^1.1.29",
    "@types/node": "^14.14.6",
    "eslint": "^7.12.1",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "redos",
    "regexp",
    "security"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "lib/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
